using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplicationInterface
{
    class Program
    {
        static void Main(string[] args)
        {
        }
    
}
public interface ISmartfone
    {
        void OS();
        void AppStore();
    }
    public class Samsung : ISmartfone
    {
        public void AppStore()
        {
            Console.WriteLine("Samsung AppStore");
        }

        public void OS()
        {
            Console.WriteLine("Samsung OS");
        }
    }
    public class IPhone : ISmartfone
    {
        public void AppStore()
        {
            Console.WriteLine("IPhone AppStore");
        }

        public void OS()
        {
            Console.WriteLine("IPhone OS");
        }
    }
}
